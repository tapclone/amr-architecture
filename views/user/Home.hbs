<style>
/* Apply styles to the full-screen component */
.fullscreen-component {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    /* Background image URL and additional background properties */
    background-image: url('/images/banner.png'); /* Replace 'your-image-url.jpg' with the actual image URL */
    background-color: #5c7065; /* Fallback background color */
    background-size: cover;
    /* Adjust as needed to cover the entire element */
    background-repeat: no-repeat; /* Prevent image from repeating */
    color: #fff; /* Change to your desired text color */
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    transform: translateY(0); /* Start it off the viewport */
    transition: transform 1s ease-in; /* Only exit animation */
    z-index: 2;
}


.fullscreen-component img {
    width: 20%;
    animation: slideIn 1s ease-in-out forwards; /* Slide in animation */
    opacity: 0; /* Initially set the opacity to 0 */
}

@keyframes slideIn {
    0% {
        transform: translateY(-50px);
        opacity: 0;
    }
    100% {
        transform: translateY(0);
        opacity: 1;
    }
}
.fullscreen-component p{
    color: #fff;
    font-size: 1.5rem;
    padding: 0 0;
      overflow: hidden; /* Ensures the content is not revealed until the animation */
  border-right: .15em solid rgb(247, 246, 221); /* The typwriter cursor */
  white-space: nowrap; /* Keeps the content on a single line */
  margin: 0 auto; /* Gives that scrolling effect as the typing happens */
  letter-spacing: .15em; /* Adjust as needed */
  animation: 
    typing 3s steps(30, end),
    blink-caret .75s step-end infinite;
}
@keyframes typing {
  from { width: 0 }
  to { width:20% }
}
@media(max-width:480px){
    .fullscreen-component img{
        width: 70%;
    }
    .fullscreen-component p{
          animation: 
    typing 3s steps(30, end),
    blink-caret .75s step-end infinite;
    }
    @keyframes typing {
  from { width: 0 }
  to { width:70% }
}
}

/* The typewriter cursor effect */
@keyframes blink-caret {
  from, to { border-color: transparent }
  50% { border-color: rgb(255, 237, 203); }
}
/* Apply styles to the homepage */
.homepage {
    opacity: 0;
    transition: opacity 1s ease-in-out;
}

    </style>
