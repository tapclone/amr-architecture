        document.addEventListener("DOMContentLoaded", function () {
            // Get references to the components
            const fullscreenComponent = document.querySelector(".fullscreen-component");
            const homepage = document.querySelector(".homepage");

            // Hide the homepage initially
            homepage.style.opacity = "0";


            // Show the full-screen component immediately
            fullscreenComponent.style.transform = "translateY(0)";

            // Slide the full-screen component upwards and simultaneously fade in the homepage
            setTimeout(function () {
                fullscreenComponent.style.transform = "translateY(-100%)";
                homepage.style.opacity = "1";
            }, 3300); // Adjust the delay as needed for the full-screen display
        });

    </script>

